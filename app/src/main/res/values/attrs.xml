<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgress">
    <attr name="color1" format="reference|color" />
    <attr name="color2" format="reference|color" />
    <attr name="color3" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="ColorPreference">
        <attr name="color" />
    </declare-styleable>
    <declare-styleable name="AppPreferenceCardView">
        <attr name="preferenceType" format="enum">
            <enum name="customTabProvider" value="0" />
            <enum name="secondaryBrowser" value="1" />
            <enum name="favoriteShare" value="2" />
        </attr>
    </declare-styleable>
</resources>